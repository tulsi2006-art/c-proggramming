#include <stdio.h>

int main() {
    int number;

    // Read the two-digit number
    scanf("%d", &number);

    // Calculate the sum of its digits
    int sum_of_digits = (number / 10) + (number % 10);

    // Output the result in the required format
    printf("Bird said:%d\n", number);
    printf("Alice must go in path-%d\n", sum_of_digits);

    return 0;
