#include<stdio.h>
int main()
    {
    int day;
    scanf(" %d", &day);
    switch(day)
        {
        
        case 1:
    {
        printf ("Monday");
        break;
        }
        case 7:
    {
        printf ("Sunday");
        break;
        }
            case 14:
            {
                printf("Enter a valid Input");
                }
}
    return 0;
    }
