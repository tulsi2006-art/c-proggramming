#include <stdio.h>

int main() {

    int num, sum = 0,temp;
    scanf("%d", &num);
    if (num >= 100) 
    {
        for (temp = num; temp > 0; temp = temp / 10)
        {
           sum += temp % 10; 
        }
        printf("Sum of digit is %d\n", sum);
    } 
    else
    {
        printf("Invalid Input\n");
    }
    return 0;
}
